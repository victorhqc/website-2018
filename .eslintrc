{
  "extends": "airbnb",
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "shared-node-browser": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": [ "Link" ],
        "specialLink": [ "to", "hrefLeft", "hrefRight" ],
        "aspects": [ "noHref", "invalidHref", "preferButton" ]
      }
    ]
  }
}
